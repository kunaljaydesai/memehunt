style.
	.page{
		font-family: 'Open Sans', sans-serif;
	}
	#upload {
		width: auto;
		float: right;
		height: auto;
	}
	.logo {
		width: 25%;
		height: auto;
		float: left;
		position: relative;

	}
	div.center iframe{
	    display: block;
	    margin-left: auto;
	    margin-right: auto;
	}
	ul {
		padding-right: 10px;
	}
	div.nav{
		width:100%;
		height: 50px;

	}
	div.header{
		padding-top: 10px;
		text-align: center;
		background-color: rgba(156,39,176,0.9);
	}
	#upload{
		float: right;
		width: auto;
		height: auto;
		margin-top: 20px;
	}
	.thumbnail{
		margin-right: 30px;
	}
html(lang='en')
	div(class='page')
			div(class="header" )
				div(class='container')
					a(href="/index")
						img(src="/images/doublie-logo@2x.png", class='logo')

			block content
		
					
					

					
			